package Class

class Person{
  private var name = ""
  private var age = 0
  def Person(name: String, age: Int){

    this.age = age
    this.name = name
  }
  def show(){
    println("name : " + name +  "\n num : " + age)
  }
}

class Student(_name : String, _age : Int, _num : Int) extends Person(_name, _age){
  private var name = _name
  private var age = _age
  private var num = _num
  
}

