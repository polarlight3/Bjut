package Class

class Person{
  private var name = ""
  private var age = 0
  def Person(name: String, age: Int){
    this.age = age
    this.name = name
  }
  def show(){
    println("name : " + name +  "\n num : " + age)
  }
}

class Student extends Person{
  private var name = ""
  private var age = 0
  private var num = 0
  def Student(name: String, age: Int, num: Int){
    super(name, age)
    this.num = num
  }
  
}

