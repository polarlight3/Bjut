
object Comput {
  def main(args: Array[String]): Unit = {
       for(i <- 1 to 5) print(i + " ") 
       for(i <- 1 until 5) print(i + " ") 
       val l1 = for(c <- "Hello"; i <- 0 to 1) yield (c + i).toChar
       val l2 = for(i <- 0 to 1; c <- "Hello") yield (c + i).toChar
       println(l1)
       println(l2)
  }
  
}