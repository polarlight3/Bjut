package Spark
import scala.collection.mutable._

object Mapp {
  def main(args: Array[String]): Unit = {
    val sc:Map[String, Int] = Map("kobe" -> 24, "paul" -> 3, "james" -> 23)
   // var sc2 = Map((10, "Alice"), (3, "Bob"), (8, "Cindy"))
    for(a <- sc)   println(a)
    println(sc("kobe"))
    
    
  }
}