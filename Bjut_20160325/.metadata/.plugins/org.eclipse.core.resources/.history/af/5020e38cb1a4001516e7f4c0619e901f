package Class

class Person(nam: String, nu: Int ){
  private var name = nam
  private var num = nu
  def show(){
    println("name : " + name +  "\n num : " + num)
  }
}

object Extends extends Enumeration {
  val Red, Yellow = Value
  def doWhat(color:Value){
    if(color == Red) println("red")
    else print("lose")
  }
  def main(args: Array[String]): Unit = {
      doWhat(Red)
  }
}