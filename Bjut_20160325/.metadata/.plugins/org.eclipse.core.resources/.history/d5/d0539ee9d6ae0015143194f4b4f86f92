
import scala.collection.mutable.ArrayBuffer

object Comput {
  def sum(pa:Int *) = {
    var result = 0
    for(i <- pa) result += i
    result
  }
  
  def main(args: Array[String]): Unit = {
   //ln(sum(1,2,3,6,5,4,7))     
    lazy val file = scala.io.Source.fromFile("/asdf")
    val b = ArrayBuffer[Int]() //
    b += 1
    println(b)
    b += (2,3,4)
    println(b)
    b ++= Array(4,5,6)
    println(b)
    b.trimEnd(2)
    println(b)
    for(i <- 0 to (10, 2))
      printf("%d ", i)
    println()
    for(i <- (0 to 10).reverse)
      printf("%d ", i)
    
  }
  
}