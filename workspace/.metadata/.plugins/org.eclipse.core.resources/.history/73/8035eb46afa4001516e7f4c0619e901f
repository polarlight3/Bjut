package Class

class Person{
  private var name = ""
  private var num = 0
  
  def this(name: String, num: Int){
    this()
    this.name = name
    this.num = num
    
  }
}

object Extends extends Enumeration {
  val Red, Yellow = Value
  def doWhat(color:Value){
    if(color == Red) println("red")
    else print("lose")
  }
  def main(args: Array[String]): Unit = {
      doWhat(Red)
  }
}