package Spark
import scala.collection.mutable._

object Mapp {
  def main(args: Array[String]): Unit = {
    val sc:Map[String, Int] = Map("Alice" -> 10, "Bob" -> 3, "Cindy" -> 8)
    var sc2 = Map((10, "Alice"), (3, "Bob"), (8, "Cindy"))
    for(a <- sc)   println(a)
    println()
    for(a <- sc2)   println(a)
    println(sc2(3))
  }
}