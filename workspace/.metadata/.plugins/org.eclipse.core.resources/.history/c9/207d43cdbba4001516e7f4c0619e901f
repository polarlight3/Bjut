
package Class

abstract class person{
  def id: Unit //abstract method
  val num: Int //abstract parameter
  var name: String
  def show(){
    println("I'm kobe")
  }
}

class student extends person{
  var num = 23
  def id = println("where amazing happen")
  override def show(){
    println("I'm lebron")
  }
}
object Override extends App {
 
}